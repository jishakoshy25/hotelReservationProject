<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="BasicTestSuite">
<listeners>
<listener class-name="com.hotel.listener.ExtentReportListener"/>
</listeners>
<test name="Test" parallel="classes" thread-count="5">
	<classes>
		<class name="com.hotel.webtests.LoginTest" />
		<class name="com.hotel.webtests.LogoutTest" />
		<class name="com.hotel.webtests.MyPageTest" />
		<class name="com.hotel.webtests.ReservationPageTest" />
		<class name="com.hotel.webtests.ReservationPlanTest" />
		<class name="com.hotel.webtests.ReservationTest" />
		<class name="com.hotel.webtests.SignUpTest" />
	</classes>
</test>
</suite>